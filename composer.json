{
    "name": "rosamarsky/laravel-command-bus",
    "version": "1.0.2",
    "description": "Simple Command Bus for Laravel framework",
    "keywords": [
        "laravel",
        "bus",
        "command bus",
        "simple sqrs",
        "clear controllers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Samarsky",
            "email": "rosamarsky@gmail.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rosamarsky\\CommandBus\\CommandBusServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/container": "*",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "sebastian/version": "^1.0.1",
        "mockery/mockery": "0.9.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Rosamarsky\\CommandBus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rosamarsky\\CommandBus\\Tests\\": "tests"
        }
    }
}
